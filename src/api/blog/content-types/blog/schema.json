{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "blog"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "subtitle": {
      "type": "string"
    },
    "displayDate": {
      "type": "datetime"
    },
    "body": {
      "type": "blocks"
    },
    "featuredImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "hideTopFeaturedImage": {
      "type": "boolean"
    },
    "duration": {
      "type": "integer"
    },
    "metaTitle": {
      "type": "string"
    },
    "metaDescription": {
      "type": "string"
    },
    "canonicalUrl": {
      "type": "string"
    },
    "notice": {
      "type": "blocks"
    },
    "noIndex": {
      "type": "boolean"
    },
    "authors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::author.author",
      "mappedBy": "blogs"
    },
    "primaryCategory": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "blogs"
    },
    "secondaryCategories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "secondaryForBlogs"
    },
    "relatedByArticles": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::blog.blog",
      "inversedBy": "relatedArticles"
    },
    "relatedArticles": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::blog.blog",
      "mappedBy": "relatedByArticles"
    },
    "references": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::reference.reference",
      "inversedBy": "blogs"
    }
  }
}
